{
    "gitbook": "3.x.x",
    "plugins": [ "lightbox","accordion","latex-codecogs", "github", "-lunr", "-search",
        "search-plus", "github-buttons",
        "tbfed-pagefooter", "expandable-chapters-small",
        "sitemap","pageview-count","theme-comscore","theme-fexa"
        ],
    "pluginsConfig": {
        "theme-fexa":{
            "search-placeholder":"输入关键字搜索", 
            "logo":"./logo.png", 
            "favicon": "./favicon.ico" 
        },
        "sitemap": {
            "hostname": "http://GUIDE-FOR-FRESHMAN-IN-Hohai-University.com/"
        },
        "theme-default": {
            "showLevel": true
        },
        "fontSettings": {
            "family": "serif",
            "size": 2
        },
        "github": {
            "url": "https://github.com/Avoidman2233"
        },
        "github-buttons": {
            "buttons": [{
                "user": "Avoidman2233",
                "repo": "GUIDE-FOR-FRESHMAN-IN-Hohai-University",
                "type": "star",
                "size": "small",
                "count": true
            }]
        },
        "tbfed-pagefooter": {
            "copyright": "Copyright &copy <a 'target='_blank'>河海大学不完全入学指北编辑部</a> 2024",
            "modify_label": "修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "anchor-navigation-ex": {
            "showLevel": true,
            "associatedWithSummary": false,
            "printLog": false,
            "multipleH1": true,
            "mode": "float",
            "showGoTop": true,
            "float": {
                "floatIcon": "fa fa-navicon",
                "showLevelIcon": false,
                "level1Icon": "fa fa-hand-o-right",
                "level2Icon": "fa fa-hand-o-right",
                "level3Icon": "fa fa-hand-o-right"
            },
            "pageTop": {
                "showLevelIcon": false,
                "level1Icon": "fa fa-hand-o-right",
                "level2Icon": "fa fa-hand-o-right",
                "level3Icon": "fa fa-hand-o-right"
            }
        },
    "pdf": {
        "pageNumbers": true,
        "fontFamily": "Arial",
        "fontSize": 12,
        "paperSize": "a4",
        "margin": {
            "right": 62,
            "left": 62,
            "top": 56,
            "bottom": 56
        }
    },
    "title": "河海大学不完全入学指北",
    "description": "做最真诚的指南",
    "author": "FACK_NOICE_PUBLIC",
    "language": "zh-hans",
    "root": ".",
    "links": {
        "sidebar": {
            "GitHub": "https://github.com/Avoidman2233"
        }
    }
}
}